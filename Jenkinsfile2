pipeline {
    agent any
    environment {
        MAVEN_HOME = "C:\\Program Files\\apache-maven-3.9.8"
        PATH = "${MAVEN_HOME}\\bin;${env.PATH}"
    }
    stages {
        stage('Build') {
            steps {
                // Maven package komutunu çalıştırın
                bat 'mvn clean package'
            }
        }
    }
}
